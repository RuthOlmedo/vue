then((response) => response.json())\r\n          .then((data) => {\r\n            this.tasks = data.todos;\r\n          });\r\n      },\r\n  \r\n      toggleTaskCompletion(task) {\r\n        task.completed = !task.completed;\r\n      },\r\n  \r\n      deleteTask(task) {\r\n        this.tasks = this.tasks.filter((t) => t.id !== task.id);\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Estilos personalizados */\r\n  </style>\r\n  "],"mappings":";;AAsBA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA;MACAC,KAAA,gCACAC,IAAA,CAAAC,QAAA,IAAAA,QAAA,CAAAC,IAAA,IACAF,IAAA,CAAAL,IAAA;QACA,KAAAC,KAAA,GAAAD,IAAA,CAAAQ,KAAA;MACA;IACA;IAEAC,qBAAAC,IAAA;MACAA,IAAA,CAAAC,SAAA,IAAAD,IAAA,CAAAC,SAAA;IACA;IAEAC,WAAAF,IAAA;MACA,KAAAT,KAAA,QAAAA,KAAA,CAAAY,MAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,EAAA,KAAAL,IAAA,CAAAK,EAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}